{
    "name": "mocniak/sorted-linked-list",
    "description": "Library with sorted linked list",
    "license": "Unlicense",
    "type": "library",
    "keywords": [
        "list"
    ],
    "authors": [
        {
            "name": "mocniak",
            "email": "karol@mocniak.pl",
            "homepage": "https://mocniak.pl"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "ramsey/devtools": "^2.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Mocniak\\SortedLinkedList\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mocniak\\Test\\SortedLinkedList\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ramsey/composer-repl": true,
            "phpstan/extension-installer": true,
            "ergebnis/composer-normalize": true,
            "captainhook/plugin-composer": true,
            "ramsey/devtools": true
        },
        "sort-packages": true
    },
    "extra": {
        "captainhook": {
            "force-install": true
        },
        "ramsey/conventional-commits": {
            "configFile": "conventional-commits.json"
        },
        "ramsey/devtools": {
            "command-prefix": "dev",
            "memory-limit": "-1"
        }
    }
}
